    <script>
        let ws;
        let beepAudio = new Audio('audio/beep.mp3'); // Load your beep audio file
        let endAudio = new Audio('audio/end.mp3'); // Load your end audio file
        let beepInterval;

        window.onload = () => {
            // Establish WebSocket connection
            ws = new WebSocket('ws://localhost:8081');

            ws.onmessage = (event) => {
                const data = JSON.parse(event.data);
                console.log("Received data:", data);

                if (Array.isArray(data)) {
                    displayResults(data); // Only call if `data` is the results array
                }

                const timeLeft = data.timeLeft.toString().padStart(2, '0'); // Ensure two digits
                const firstDigit = timeLeft[0]; // First digit
                const secondDigit = timeLeft[1]; // Second digit

                // Update displayed digits
                document.getElementById('firstDigit').innerText = firstDigit;
                document.getElementById('secondDigit').innerText = secondDigit;

                // Update cycle ID
                document.getElementById('cycleId').innerText = data.cycleId;

                // Update class names based on the current digits
                const firstDiv = document.getElementById('firstDigitDiv');
                const secondDiv = document.getElementById('secondDigitDiv');
                firstDiv.className = `n${firstDigit}`;
                secondDiv.className = `n${secondDigit}`;

                // Handle disabling choice and overlay between 5 and 1 seconds
                handleChoiceAndOverlay(data.timeLeft);
            };

            ws.onclose = () => {
                console.log('WebSocket connection closed');
            };

            // Function to handle disabling choice and showing overlay
            function handleChoiceAndOverlay(timeLeft) {
                let choice = document.getElementById("choose-choice");
                const overlayTimer = document.getElementById('overlayTimer');

                if (timeLeft > 0 && timeLeft <= 5) {
                    choice.classList.add("disabled");
                    overlayTimer.innerText = timeLeft;
                    overlayTimer.style.display = 'block';

                    if (!beepInterval) {
                        beepInterval = setInterval(() => {
                            beepAudio.currentTime = 0; // Reset audio to start
                            beepAudio.play();
                        }, 1000); // Play beep every second
                    }
                } else if (timeLeft === 0) {
                    clearInterval(beepInterval); // Stop beep sound
                    beepInterval = null; // Reset interval
                    endAudio.play(); // Play end sound
                    overlayTimer.style.display = 'none';
                    choice.classList.remove("disabled");
                } else {
                    // Clear beep interval if the time is above 5 seconds
                    clearInterval(beepInterval);
                    beepInterval = null;
                }
            }

            // Function to display game results (only recent 10)
            function displayResults(results) {
                const resultsDiv = document.getElementById('results');

                // Ensure results is an array
                if (!Array.isArray(results)) {
                    console.error('Expected results to be an array, but received:', results);
                    return;
                }

                resultsDiv.innerHTML = ''; // Clear previous results

                results.forEach(result => {
                    let number;
                    let mixedColorClass = result.colorResult === 'violet' && result.bigSmallResult === 'small' ? 'mixedColor0' : 'mixedColor5';
                    
                    if (result.colorResult === 'violet') {
                        // Handle case for violet color with different numbers
                        number = result.bigSmallResult === 'small' ? 0 : 5;
                        let originClasses = `
                             <div data-v-481307ec="" class="GameRecord__C-origin-I ${number === 0 ? 'red' : 'green'}"></div>
                             <div data-v-481307ec="" class="GameRecord__C-origin-I ${result.colorResult}"></div>`;

                        resultsDiv.innerHTML += `
                <div data-v-481307ec="" class="van-row">
                    <div data-v-481307ec="" class="van-col van-col--8">${result.currCycleId}</div>
                    <div data-v-481307ec="" class="van-col van-col--5 numcenter">
                        <div data-v-481307ec="" class="GameRecord__C-body-num ${mixedColorClass}">${result.numberResult}</div>
                    </div>
                    <div data-v-481307ec="" class="van-col van-col--5">
                        <span data-v-481307ec="">${result.bigSmallResult}</span>
                    </div>
                    <div data-v-481307ec="" class="van-col van-col--6">
                        <div data-v-481307ec="" class="GameRecord__C-origin">
                            ${originClasses}
                        </div>
                    </div>
                </div>`;
                    } else {
                        // Handle other colors
                        resultsDiv.innerHTML += `
                <div data-v-481307ec="" class="van-row">
                    <div data-v-481307ec="" class="van-col van-col--8">${result.currCycleId}</div>
                    <div data-v-481307ec="" class="van-col van-col--5 numcenter">
                        <div data-v-481307ec="" class="GameRecord__C-body-num ${result.colorResult}">${result.numberResult}</div>
                    </div>
                    <div data-v-481307ec="" class="van-col van-col--5">
                        <span data-v-481307ec="">${result.bigSmallResult}</span>
                    </div>
                    <div data-v-481307ec="" class="van-col van-col--6">
                        <div data-v-481307ec="" class="GameRecord__C-origin">
                            <div data-v-481307ec="" class="GameRecord__C-origin-I ${result.colorResult}"></div>
                        </div>
                    </div>
                </div>`;
                    }
                });
            }
        }
    </script>

